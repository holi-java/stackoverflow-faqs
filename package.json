{
  "name": "stackoverflow-faqs",
  "version": "1.0.0",
  "description": "answers of the site http://www.stackoverflow.com",
  "scripts": {
    "test": "jest @.*?[.]test[.]tsx?$",
    "test:all": "jest [.]test[.]tsx?$ | exit 0",
    "clean": "rm ./build -rf",
    "tsc": "npm run clean & tsc"
  },
  "keywords": [
    "stackoverflow",
    "typescript",
    "javascript"
  ],
  "author": "holi-java",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^19.2.1",
    "@types/reflect-metadata": "0.0.5",
    "jest": "^19.0.2",
    "reflect-metadata": "^0.1.10",
    "ts-jest": "19.0.0",
    "tslib": "^1.6.0",
    "typescript": "2.2.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test/bootstrap",
    "testRegex": "[.]test[.](tsx?|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
