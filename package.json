{
  "name": "stackoverflow-faqs",
  "version": "1.0.0",
  "description": "answers of the site http://www.stackoverflow.com",
  "scripts": {
    "test": "jest @.*?[.]test[.]tsx?$",
    "test:all": "jest | exit 0"
  },
  "keywords": [
    "stackoverflow",
    "typescript",
    "javascript"
  ],
  "author": "holi-java",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^19.2.1",
    "jest": "^19.0.2",
    "ts-jest": "19.0.0",
    "typescript": "2.2.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test/bootstrap",
    "testRegex": "[.]test[.]tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
